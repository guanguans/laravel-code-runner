{
    "name": "guanguans/package-skeleton",
    "description": "A PHP package template repository. - 一个PHP软件包模板存储库。",
    "license": "MIT",
    "type": "library",
    "abandoned": false,
    "version": "1.0.7",
    "non-feature-branches": [],
    "keywords": [
        "package",
        "template",
        "PHP",
        "skeleton"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "guanguans",
            "email": "ityaozm@gmail.com",
            "homepage": "https://www.guanguans.cn",
            "role": "developer"
        }
    ],
    "homepage": "https://github.com/guanguans/package-skeleton",
    "support": {
        "issues": "https://github.com/guanguans/package-skeleton/issues",
        "source": "https://github.com/guanguans/package-skeleton"
    },
    "funding": [
        {
            "type": "wechat",
            "url": "https://www.guanguans.cn/images/wechat.jpeg"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.8",
        "code-lts/doctum": "^5.4",
        "codedungeon/phpunit-result-printer": "^0.31 || ^0.32",
        "dms/phpunit-arraysubset-asserts": "^0.3 || ^0.4",
        "ekino/phpstan-banned-code": "^1.0",
        "ergebnis/phpstan-rules": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.4",
        "infection/infection": "^0.26",
        "jetbrains/phpstorm-attributes": "^1.0",
        "johnkary/phpunit-speedtrap": "^4.0",
        "kylekatarnls/update-helper": "^1.2",
        "marcocesarato/php-conventional-changelog": "^1.14",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.3",
        "nunomaduro/larastan": "^1.0",
        "nyholm/nsa": "^1.3",
        "orchestra/testbench": "^5.0 || ^6.0 || ^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phly/keep-a-changelog": "^2.10",
        "phpbench/phpbench": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "povils/phpmnd": "^2.5 || ^3.0",
        "rector/rector": "^0.14",
        "roave/no-floaters": "^1.6",
        "roave/security-advisories": "dev-latest",
        "spatie/pest-plugin-snapshots": "^1.1",
        "symplify/monorepo-builder": "^10.0",
        "vimeo/psalm": "^4.16"
    },
    "replace": {},
    "conflict": {},
    "provide": {},
    "suggest": {},
    "repositories": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {},
        "psr-4": {
            "Guanguans\\PackageSkeleton\\": "src"
        },
        "classmap": [],
        "files": [],
        "exclude-from-classmap": [
            "benchmarks"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Guanguans\\PackageSkeletonTests\\": "tests"
        }
    },
    "bin": [],
    "archive": {},
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "kylekatarnls/update-helper": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "apcu-autoloader": true,
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "hooks": {
            "post-merge": [
                "composer checks"
            ],
            "pre-commit": [
                "composer checks"
            ]
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        },
        "update-helper": "Guanguans\\PackageSkeleton\\UpdateHelper"
    },
    "scripts": {
        "post-install-cmd": [
            "@cghooks add --ignore-lock",
            "@cghooks update"
        ],
        "post-update-cmd": [
            "@cghooks update"
        ],
        "post-autoload-dump": [
            "UpdateHelper\\UpdateHelper::check"
        ],
        "benchmark": "./vendor/bin/phpbench run --retry-threshold=3 --iterations=15 --revs=1000 --warmup=2",
        "cghooks": "./vendor/bin/cghooks",
        "changelog": "./vendor/bin/conventional-changelog",
        "changelog-keep": "./vendor/bin/keep-a-changelog",
        "changelog-major": "@changelog --major",
        "changelog-minor": "@changelog --minor",
        "changelog-patch": "@changelog --patch",
        "checks": [
            "@mark-start",
            "@style-lint",
            "@mark-separate",
            "@test",
            "@mark-finish"
        ],
        "doctum": "./vendor/bin/doctum.php update doctum.php",
        "infection": "./vendor/bin/infection --test-framework=pest --show-mutations --threads=4 --ansi",
        "mark-finish": "printf '\\n!\\n!\\t\\033[0;32m%s\\033[0m\\n!\\n\\n' \"Finished\"",
        "mark-separate": "printf '\\n!\\n!\\t\\033[0;33m%s\\033[0m\\n!\\n\\n' \"----------------\"",
        "mark-start": "printf '\\n!\\n!\\t\\033[0;36m%s\\033[0m\\n!\\n\\n' \"Started\"",
        "md-fix": "@md-lint --fix",
        "md-lint": "lint-md --config .lintmdrc ./*.md ./.github/ ./docs/",
        "pest": "./vendor/bin/pest --cache-result-file=./build//phpunit/.phpunit.result.cache",
        "pest-coverage": "@pest --coverage-html=./build/phpunit/ --coverage-clover=clover.xml --coverage",
        "phpmnd": "./vendor/bin/phpmnd ./src/ --exclude=./vendor/ --non-zero-exit-on-violation",
        "phpstan": "./vendor/bin/phpstan analyse -vv",
        "phpstan-baseline": "@phpstan --generate-baseline",
        "post-merge": [
            "composer install"
        ],
        "psalm": "./vendor/bin/psalm",
        "psalm-baseline": "@psalm --update-baseline",
        "rector": "./vendor/bin/rector --clear-cache --ansi -v",
        "rector-dry-run": "@rector --dry-run",
        "release": "./vendor/bin/monorepo-builder release --ansi -vvv",
        "style-fix": "./vendor/bin/php-cs-fixer fix --using-cache=no --config=.php-cs-fixer.php --ansi",
        "style-lint": "@style-fix --diff --dry-run",
        "test": "@pest",
        "test-coverage": "@pest-coverage"
    },
    "scripts-descriptions": {}
}
