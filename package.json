{
    "name": "laravel-code-runner",
    "author": "guanguans <ityaozm@gmail.com>",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/guanguans/laravel-code-runner.git"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "style-fix": "prettier --write 'resources/**/*.{css,js,vue,php}' '*.js'"
    },
    "dependencies": {
        "@ungap/custom-elements": "^1.1.1",
        "axios": "^1.1.3",
        "bootstrap": "^5.2.2",
        "codemirror": "^6.0.1",
        "highlighted-code": "^0.3.7",
        "jquery": "^3.6.1",
        "laravel-mix": "^6.0.49",
        "tailwindcss": "^3.1.6"
    },
    "devDependencies": {
        "prettier": "^2.7.1"
    }
}
