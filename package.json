{
    "name": "laravel-code-runner",
    "author": "guanguans <ityaozm@gmail.com>",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/guanguans/laravel-code-runner.git"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "eslint": "eslint resources/ --max-warnings=0",
        "esfix": "eslint resources/ --max-warnings=0 --fix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "prettier": "prettier --write 'resources/**/*.{css,js,vue,blade.php}' '*.js'",
        "vite": "vite",
        "vite-build": "vite build"
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.5.3",
        "@ungap/custom-elements": "^1.1.1",
        "@vitejs/plugin-vue": "^3.2.0",
        "alpinejs": "^3.10.5",
        "axios": "^1.1.3",
        "bootstrap": "^5.2.2",
        "codemirror": "^6.0.1",
        "highlighted-code": "^0.3.7",
        "laravel-mix": "^6.0.49",
        "laravel-vite-plugin": "^0.7.0",
        "tailwindcss": "^3.1.6",
        "vite": "^3.2.4"
    },
    "devDependencies": {
        "@shufo/prettier-plugin-blade": "^1.8.0",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/typescript-estree": "^5.43.0",
        "eslint": "^8.27.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.5.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vue": "^9.7.0",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "typescript": "^4.9.3"
    }
}
